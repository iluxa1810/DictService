  <configSections>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="WCFConsoleServer.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    </sectionGroup>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
  </configSections>
  <connectionStrings>
    <add name="WCFConsoleServer.Properties.Settings.DictServiceDB" connectionString="Data Source=octopus-elar2;Initial Catalog=DictService;Integrated Security=True" providerName="System.Data.SqlClient" />
    <add name="DictServiceEntities" connectionString="metadata=res://*/Models.DictServiceModel.csdl|res://*/Models.DictServiceModel.ssdl|res://*/Models.DictServiceModel.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=octopus-elar2;initial catalog=DictService;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" />
  </connectionStrings>
  <system.serviceModel>
    <services>
      <service behaviorConfiguration="DictionaryServiceBehavior" name="WCFConsoleServer.Services.DictionaryService">
        <endpoint address="upload" binding="basicHttpBinding" bindingConfiguration="FileTransferServiceBinding" contract="WCFConsoleServer.Contracts.IFileUpload" />
        <endpoint address="download" binding="basicHttpBinding" bindingConfiguration="FileTransferServiceBinding" contract="WCFConsoleServer.Contracts.IFileDownload" />
        <endpoint address="Data" binding="basicHttpBinding" contract="WCFConsoleServer.Contracts.IData" />
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
      </service>
    </services>
    <behaviors>
