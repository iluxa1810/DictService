      </basicHttpBinding>
    </bindings>
  </system.serviceModel>
  <applicationSettings>
    <WCFConsoleServer.Properties.Settings>
      <setting name="DictionaryRepo" serializeAs="String">
        <value>D:\Dictionary</value>
      </setting>
    </WCFConsoleServer.Properties.Settings>
  </applicationSettings>
  <startup>
